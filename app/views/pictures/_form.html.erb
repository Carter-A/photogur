<%= link_to "Back to Pictures", pictures_url, class: "mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect" %>
<%= form_for @picture do |f| %>
  <div class="mdl-textfield mdl-js-textfield">
    <%= f.label :artist, class: "mdl-textfield__label" %><br>
    <%= f.text_field :artist, class: "mdl-textfield__input" %>
  </div>
  <div class="mdl-textfield mdl-js-textfield">
    <%= f.label :title, class: "mdl-textfield__label" %><br>
    <%= f.text_field :title, class: "mdl-textfield__input" %>
  </div>
  <div class="mdl-textfield mdl-js-textfield">
    <%= f.label :url, class: "mdl-textfield__label" %><br>
    <%= f.text_field :url, class: "mdl-textfield__input" %>
  </div>
    <%= f.submit "Save", class: "mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect"  %>

<% end %>
